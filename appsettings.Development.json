{
  "Auth": {
    "Jwt": {
      "Issuer": "http://localhost:27240/",
      "Audience": "http://localhost:27240/",
      "Key": "---insert-your-own-key-here---",
      "TokenExpirationInMinutes": 86400
    }
  },
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Debug",
        "System": "Information",
        "Microsoft": "Information"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Debug",
        "System": "Information",
        "Microsoft": "Information"
      }
    }
  },
  "StaticFiles": {
    "Headers": {
      "Cache-Control": "no-cache, no-store",
      "Pragma": "no-cache",
      "Expires": "-1"
    }
  }
}
